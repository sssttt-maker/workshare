<h1>Room新規作成</h1>

<%= form_with model: @room, local: true do |f| %>
  <div class="form-group">
    <%= f.label :name %>
    <%= f.text_field :name, class: 'form-control', id: 'room_name' %>
    <%= f.hidden_field :organization_id, value: current_organization.id %>
  </div>
  <%= f.submit nil, class: 'btn btn-primary' %>
<% end %>
